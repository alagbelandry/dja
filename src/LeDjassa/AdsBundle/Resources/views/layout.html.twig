{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}

{% block head_meta_description %}annonces grauites cote d'ivoire{% endblock %}
{% block head_title %}Annonces gratuites Cote d'ivoire{% endblock %}

{% block head_css %}
	{{ parent() }}

	{% stylesheets
	'bundles/ledjassaads/css/color.css'
	'bundles/ledjassaads/css/screen.css'  
	 %}
	 <link media="screen" rel="stylesheet" type="text/css" href="{{ asset_url }}">
	{% endstylesheets %}

	{% stylesheets
	'bundles/ledjassaads/css/960.css'  
	 %}
	 <link media="all" rel="stylesheet" type="text/css" href="{{ asset_url }}">
	{% endstylesheets %}

	<!--[if lt IE 9]>
	<link rel="stylesheet" type="text/css" href="stylesheet/ie.css" media="screen" />
	<![endif]-->
{% endblock %}

{% block head_js %}
	{{ parent() }}

    {% javascripts 
    'bundles/ledjassaads/js/jquery-1.8.1.min.js'
    'bundles/ledjassaads/js/jquery-ui-1.8.23.custom.min.js' 
    %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}

  {% block body_outer %}
    <body class="s_layout_fixed" {{bp_body_attributes|default('')|raw}}>
      <div id="wrapper">
        {%block body%}
          {%block body_container%}
              <header id="header" class="container_12">
                {% block body_container_header %}
                  <div class="grid_12">
                    <a id="site_logo" href="">Shoppica store - Premium e-Commerce Theme</a>
                    <div id="categories" class="s_nav">
                      <ul>
                        <li> <a href="listing_1.html">ACCEUIL</a></li>
                        <li> <a href="listing_1.html">DEPOSER UNE ANNONCE</a></li>
                        <li> <a href="listing_1.html">OFFRES</a></li>
                        <li> <a href="listing_1.html">DEMANDES</a></li>
                        <li> <a href="listing_1.html">AIDE</a></li>
                      </ul>
                    </div>
                  </div>
                {% endblock %}
              </header>
              <div id="intro">
                <div id="intro_wrap">
                  <div class="container_12">
                    {% block body_container_intro %}{% endblock %}
                  </div>
                </div>
              </div>
              <div id="content" class="container_12" role="main">
                {% block body_container_main %}{% endblock %}
              </div>
              <footer>
                {% block body_container_footer %}{% endblock %}
              </footer>
          {%endblock body_container%}
          {% block body_js %}
            {% block body_js_analytics %}
              {% if bp_analytics_id is defined %}
              <script>
                var _gaq=[['_setAccount','{{ bp_analytics_id }}'],['_trackPageview']];
                (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
                  g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
                  s.parentNode.insertBefore(g,s)}(document,'script'));
              </script>
              {% endif %}
            {% endblock body_js_analytics %}
          {% endblock body_js %}
        {% endblock body %}
    </div>
    </body>
  {% endblock body_outer %}